;
; BIND data file for local loopback interface
;
$ORIGIN demo.lab.
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			2011040101      ; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.demo.lab.
dns.demo.lab	IN	CNAME	s1.p16001.lab.
