;
; BIND data file for local loopback interface
;
$ORIGIN p16002.lab.
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			2011040101      ; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.demo.lab.
s1.p16002.lab.	IN	A	10.16.2.101
s1.p16002.lab.	IN	AAAA	FD00:16:2::101
s1v4.p16002.lab.  IN      A       10.16.2.101
s1v6.p16002.lab.  IN      AAAA    FD00:16:2::101
r1.p16002.lab.  IN      A       10.16.2.11
r1.p16002.lab.  IN      AAAA    FD00:16:2::11
r1v4.p16002.lab.  IN      A       10.16.2.11
r1v6.p16002.lab.  IN      AAAA    FD00:16:2::11
